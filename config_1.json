{
  "GarageDoorPi": {
    "name": "garage door monitor",
    "database_path": "/home/pi/projects/gdoor/garage.db",
    "database_table": "door_state",
    "digital_io": [
       { 
         "type": "input",
         "name": "up_position", 
         "pin": 0,
         "resistor_mode": "pullup"
       },
       { 
         "type": "input",
         "name": "down_position", 
         "pin": 1
       },
       { 
         "type": "output",
         "name": "door_cycling", 
         "pin": 3
       }
    ],
    "loop_time_ms": 2000
  }
}
